<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team1.project.dao.ChatMemberDAO">

   <insert id="registerChatMember" >
        insert into CHATTER
        (MEMBER_ID, ROOM_NUM, JOIN_TIME)
        <foreach collection="memberIds" item="item" separator="UNION ALL" >
        SELECT #{item} , #{chatRoomNum} , sysdate FROM DUAL
        </foreach>
    </insert>


</mapper>
 